        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventCalendar\\": "tests/EventCalendar"
        }
    },
    "replace": {
        "jaroslav-kubicek/event-calendar": ">=0.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
